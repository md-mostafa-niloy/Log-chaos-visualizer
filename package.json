{
  "name": "log-chaos-visualizer",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "format": "prettier --write .",
    "gen:logs:20k": "python3 generate_logs.py --lines 20000 --output ./public/data/generated-20000.log",
    "gen:logs:50k": "python3 generate_logs.py --lines 50000 --output ./public/data/generated-50000.log",
    "gen:logs:100k": "python3 generate_logs.py --lines 100000 --output ./public/data/generated-100000.log",
    "gen:logs:200k": "python3 generate_logs.py --lines 200000 --output ./public/data/generated-200000.log",
    "gen:logs:all": "npm run gen:logs:20k && npm run gen:logs:50k && npm run gen:logs:100k && npm run gen:logs:200k"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@11.6.1",
  "dependencies": {
    "@angular/cdk": "^21.0.2",
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/material": "^21.0.2",
    "@angular/platform-browser": "^21.0.0",
    "@angular/router": "^21.0.0",
    "chart.js": "^4.5.1",
    "chartjs-adapter-date-fns": "^2.0.1",
    "date-fns": "^4.1.0",
    "ng2-charts": "^8.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.0.1",
    "@angular/cli": "^21.0.1",
    "@angular/compiler-cli": "^21.0.0",
    "angular-eslint": "21.0.1",
    "eslint": "^9.39.1",
    "jsdom": "^27.1.0",
    "typescript": "~5.9.2",
    "typescript-eslint": "8.46.4",
    "vitest": "^4.0.8",
    "prettier": "^3.4.2"
  }
}
